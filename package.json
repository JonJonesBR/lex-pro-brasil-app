{
  "name": "lex-pro-brasil",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "type": "module",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@google/genai": "^0.14.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "expo": "^54.0.9",
    "expo-font": "~14.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.14.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.9.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.1"
  },
  "build": {
    "appId": "com.lexprobrasil.app",
    "productName": "Lex Pro Brasil",
    "files": [
      "dist/**/*",
      "electron.cjs"
    ],
    "asarUnpack": [
      "!**/node_modules/fsevents/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  }
}
